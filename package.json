{
  "name": "ProjectName",
  "version": "1.0.0",
  "description": "Description of project",
  "repository": {
    "type": "git",
    "url": "git@github.com:bag-man/nodestack.git"
  },
  "author": "Authors name(s)",
  "license": "GPL-3.0",
  "main": "app/server",
  "scripts": {
    "clean": "rm -rf coverage/ && rm -rf dist/",
    "build": "npm run clean && tsc",
    "start": "node ./dist/server.js",
    "watch": "npm run clean && tsc-watch --onSuccess 'node ./dist/server.js'",
    "lint": "./node_modules/.bin/tslint --project tsconfig.json",
    "fix": "tslint --project tsconfig.json --fix",
    "test": "npm run clean && npm run lint && ./node_modules/.bin/ts-mocha -p ./tsconfig.json test/**/*.ts -- || true",
    "travis": "npm run test",
    "bisect": "git bisect run ./node_modules/.bin/ts-mocha -p ./tsconfig.json test/**/*.ts",
    "prepush": "npm test"
  },
  "engines": {
    "node": "10.9"
  },
  "devDependencies": {
    "@types/compression": "0.0.36",
    "@types/express": "^4.16.0",
    "@types/mocha": "^5.2.5",
    "@types/morgan": "^1.7.35",
    "husky": "^0.13.2",
    "mocha": "^5.2.0",
    "ts-mocha": "^2.0.0",
    "tsc-watch": "^1.0.27",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "compression": "^1.7.3",
    "express": "^4.16.3",
    "morgan": "^1.9.0"
  }
}
